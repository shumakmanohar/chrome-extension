{
	"manifest_version": 3,
	"name": "Simple Chrome Extention",
	"description": "Chrome Extention Task",
	"version": "3.0",
	"permissions": ["storage", "alarms"],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	}
}
